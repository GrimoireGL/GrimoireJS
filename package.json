{
  "name": "jThree",
  "description": "New version of jThree derived from m-mituhide jThreev2",
  "version": "0.1.0",
  "homepage": "https://github.com/jThreeJS/jThree",
  "author": {
    "name": "LimeStreem",
    "email": "LimeStreem@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jThreeJS/jThree"
  },
  "bugs": {
    "url": "https://github.com/jThreeJS/jThree/issues"
  },
  "main": "lib/jThree",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "build:bundle": "BABEL_ENV=production browserify -v -d ./lib/jThree.js -t [ babelify ] | exorcist ./product/j3.js.map > ./product/j3.js",
    "build:debug": "BABEL_ENV=production gulp build:debug",
    "build:ts": "tsc --sourcemap",
    "build:copy": "gulp build:main:others",
    "build": "npm run build:ts && npm run build:copy && npm run build:bundle",
    "watch:bundle": "BABEL_ENV=development watchify -v -d ./lib/jThree.js -t [ babelify ] -o 'exorcist ./product/j3.js.map > ./product/j3.js'",
    "watch:ts": "tsc --watch --sourcemap",
    "watch": "npm run watch:ts & npm run watch:bundle",
    "test": "BABEL_ENV=test ava --require babel-register ./test/**/*Test.js",
    "precover": "npm run build-ts -- --nosourcemap && npm run build-copy",
    "cover": "nyc --all --reporter=json --reporter=text npm run test",
    "doc": "gulp --require coffee-script/register doc",
    "clean": "gulp --require coffee-script/register clean",
    "lint": "gulp --require coffee-script/register lint:ts"
  },
  "browserify": {
    "transform": [
      ["txtify", {"extensions": [".json", ".html", ".css", ".glsl", ".xmml", ".rsml", ".xml"]}]
    ]
  },
  "directories": {
    "test": "test"
  },
  "ava": {
    "verbose": true
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "es2015",
          "stage-3"
        ]
      },
      "production": {
        "presets": [
          "es2015",
          "stage-3"
        ]
      },
      "test": {
        "presets": [
          "es2015",
          "stage-2"
        ]
      }
    }
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "coffee-loader": "^0.7.2",
    "coffee-reactify": "^4.0.0",
    "coffee-script": "^1.10.0",
    "coffeeify": "^2.0.1",
    "coveralls": "^2.11.8",
    "del": "^2.2.0",
    "difflet": "^1.0.1",
    "envify": "^3.4.0",
    "espower-babel": "^4.0.1",
    "espower-typescript": "^5.0.1",
    "exorcist": "^0.4.0",
    "gl": "^2.1.5",
    "glob-array": "^0.1.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-connect": "^3.1.0",
    "gulp-if": "^2.0.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-jade": "^1.1.0",
    "gulp-match": "^1.0.1",
    "gulp-mocha": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^4.3.3",
    "gulp-typedoc": "^1.2.1",
    "gulp-typescript": "^2.12.1",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "is-promise": "^2.1.0",
    "js-cookie": "^2.1.0",
    "js-yaml": "^3.5.5",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "node-notifier": "^4.5.0",
    "nyc": "bcoe/nyc#b5907adbd638d774629e12dc00080e7fe132c7df",
    "power-assert": "^1.3.1",
    "pretty-hrtime": "^1.0.2",
    "proxyquire": "^1.7.4",
    "radium": "^0.16.6",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-simpletabs": "^0.7.0",
    "react-tap-event-plugin": "^0.2.2",
    "react-tooltip": "^1.1.4",
    "react-treeview": "^0.4.3",
    "remap-istanbul": "^0.5.1",
    "run-sequence": "^1.1.5",
