{
  "name": "jThree",
  "description": "New version of jThree derived from m-mituhide jThreev2",
  "version": "0.1.0",
  "homepage": "https://github.com/jThreeJS/jThree",
  "author": {
    "name": "LimeStreem",
    "email": "LimeStreem@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jThreeJS/jThree"
  },
  "bugs": {
    "url": "https://github.com/jThreeJS/jThree/issues"
  },
  "main": "lib/jThree",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "start": "./node_modules/.bin/gulp --require coffee-script/register server",
    "build": "BABEL_ENV=production ./node_modules/.bin/gulp --require coffee-script/register build",
    "debug": "BABEL_ENV=production ./node_modules/.bin/gulp --require coffee-script/register build:debug",
    "watch": "BABEL_ENV=development ./node_modules/.bin/gulp --require coffee-script/register watch",
    "lib-to-lib-es6": "mv ./lib ./lib-es6",
    "copy-others-lib-to-lib-es5": "./node_modules/.bin/gulp --require coffee-script/register copy --src=./lib-es6 --dest=./lib",
    "copy-others-test-to-test-es5": "./node_modules/.bin/gulp --require coffee-script/register copy --src=./test --dest=./test-es5",
    "build-lib-es6-to-lib": "BABEL_ENV=test ./node_modules/.bin/babel ./lib-es6 --out-dir ./lib --source-maps inline",
    "build-test-to-test-es5": "BABEL_ENV=test ./node_modules/.bin/babel ./test --out-dir ./test-es5 --source-maps inline",
    "build-lib-es5": "npm run copy-others-lib-to-lib-es5 && npm run build-lib-es6-to-lib",
    "build-test-es5": "npm run copy-others-test-to-test-es5 && npm run build-test-to-test-es5",
    "test-slow": "npm run precover && npm run test-es5",
    "test-es5": "./node_modules/.bin/ava ./test-es5/**/*Test.js",
    "test": "BABEL_ENV=test ./node_modules/.bin/ava --require babel-register ./test/**/*Test.js",
    "build-ts": "./node_modules/.bin/gulp --require coffee-script/register build:main:ts",
    "copy-others-src-to-lib": "./node_modules/.bin/gulp --require coffee-script/register build:main:others",
    "precover-slow": "npm run build-ts -- --nosourcemap && npm run copy-others-src-to-lib && npm run lib-to-lib-es6 && npm run build-lib-es5 && npm run build-test-es5",
    "cover-slow": "./node_modules/.bin/nyc --all --reporter=json --reporter=text npm run test-es5 && npm run cover-slow-lcov",
    "cover-slow-lcov": "./node_modules/.bin/remap-istanbul -i ./coverage/coverage-final.json -o ./coverage/lcov-report -t lcovonly -b /Users/pnlybubbles/Devs/jThree",
    "precover": "npm run build-ts -- --nosourcemap && npm run copy-others-src-to-lib",
    "cover": "./node_modules/.bin/nyc --all --reporter=json --reporter=text npm run test",
    "precicover": "npm run build-ts -- --nosourcemap && npm run copy-others-src-to-lib",
    "cicover": "./node_modules/.bin/nyc --all npm run test && ./node_modules/.bin/nyc report --reporter=text-lcov | coveralls",
    "doc": "./node_modules/.bin/gulp --require coffee-script/register doc",
    "clean": "./node_modules/.bin/gulp --require coffee-script/register clean",
    "lint": "./node_modules/.bin/gulp --require coffee-script/register lint:ts"
  },
  "directories": {
    "test": "test"
  },
  "ava": {
    "verbose": true
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "es2015",
          "stage-3"
        ]
      },
      "production": {
        "presets": [
          "es2015",
          "stage-3"
        ]
      },
      "test": {
        "presets": [
          "es2015",
          "stage-2"
        ]
      }
    }
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "coffee-loader": "^0.7.2",
    "coffee-reactify": "^4.0.0",
    "coffee-script": "^1.10.0",
    "coffeeify": "^2.0.1",
    "coveralls": "^2.11.8",
    "del": "^2.2.0",
    "difflet": "^1.0.1",
    "envify": "^3.4.0",
    "espower-babel": "^4.0.1",
    "espower-typescript": "^5.0.1",
    "gl": "^2.1.5",
    "glob-array": "^0.1.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-connect": "^3.1.0",
    "gulp-if": "^2.0.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-jade": "^1.1.0",
    "gulp-match": "^1.0.1",
    "gulp-mocha": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^4.3.3",
    "gulp-typedoc": "^1.2.1",
    "gulp-typescript": "^2.12.1",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "is-promise": "^2.1.0",
    "js-cookie": "^2.1.0",
    "js-yaml": "^3.5.5",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "node-notifier": "^4.5.0",
    "nyc": "bcoe/nyc#b5907adbd638d774629e12dc00080e7fe132c7df",
    "power-assert": "^1.3.1",
    "pretty-hrtime": "^1.0.2",
    "proxyquire": "^1.7.4",
    "radium": "^0.16.6",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-simpletabs": "^0.7.0",
    "react-tap-event-plugin": "^0.2.2",
    "react-tooltip": "^1.1.4",
    "react-treeview": "^0.4.3",
    "remap-istanbul": "^0.5.1",
    "run-sequence": "^1.1.5",
    "shader-loader": "^1.1.4",
    "shaderify": "pnlybubbles/shaderify#0fcdf2aa0140253170cbbc608ade68d34f78e448",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.0",
    "superagent": "^1.8.1",
    "ternary-stream": "^2.0.0",
    "through2": "^2.0.1",
    "tsify": "^0.14.1",
    "tslint": "^3.6.0",
    "txtify": "jThreeJS/txtify#40854f05048e072720261010297315a543593a7e",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.9",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "xmldom": "^0.1.22",
    "yargs": "^4.3.1"
  },
  "keywords": [],
  "dependencies": {
    "gl-matrix": "^2.3.2",
    "json5": "^0.5.0",
    "lodash.isarray": "^4.0.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.isnumber": "^3.0.3",
    "lodash.isplainobject": "^4.0.3",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "object-assign": "^4.0.1",
    "q": "^1.4.1"
  }
}
